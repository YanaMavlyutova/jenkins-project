#!groovy
properties([disableConcurrentBuilds()])

pipeline {
    agent {
       label 'master'
       }
    options {
      timestamps()
  }
  stages {
    stage("First step") {
      steps {
        sh 'hostname'
      }
    }
    stage('Build Release from Tag') {
        when {
          tag 'Release*'
        }
    }
  }
}
